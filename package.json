{
  "name": "trybe-exercicios",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:HigorAnjos/trybe-exercicios.git",
  "author": "higor cavalcante dos anjos <higorc.anjos@gmail.com>",
  "license": "MIT",
  "scripts": {
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^8.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
